# Chitkara Buzz - Environment Configuration
# Copy this file to .env and fill in your actual values

# College Main API
REACT_APP_API_URL=https://api.chitkara.edu.in
REACT_APP_API_KEY=your_api_key_here

# Individual Club APIs
REACT_APP_CODE_CRUSADERS_API=https://codecrusaders.chitkara.edu.in/api/events
REACT_APP_BUSINESS_CLUB_API=https://businessclub.chitkara.edu.in/api/events
REACT_APP_CULTURAL_CLUB_API=https://culturalclub.chitkara.edu.in/api/events
REACT_APP_SPORTS_CLUB_API=https://sportsclub.chitkara.edu.in/api/events

# Firebase Configuration (if using Firebase)
REACT_APP_FIREBASE_URL=https://chitkara-events.firebaseio.com
REACT_APP_FIREBASE_DATABASE_URL=https://chitkara-events.firebaseio.com

# GraphQL Endpoint
REACT_APP_GRAPHQL_URL=https://graphql-api.chitkara.edu.in/graphql

# Scraper Backend
REACT_APP_SCRAPER_API=https://events-scraper-api.chitkara.edu.in/api/scrape-all

# App Configuration
REACT_APP_POLL_INTERVAL=30000
REACT_APP_APP_NAME=Chitkara Buzz
REACT_APP_COLLEGE_NAME=Chitkara University

# Features Toggle
REACT_APP_ENABLE_POLLING=true
REACT_APP_ENABLE_NOTIFICATIONS=false
REACT_APP_DEBUG_MODE=false
